<!--
  Only Flaps are defined.
  Lights will be ignored until a stable solution/variable can be found.
-->
<Rules>
  <Rule Simulator="Microsoft Flight" TitleContains="A320 Neo FlyByWire">
    <NavigationLights Ignored="True" />
    <BeaconLights Ignored="True" />
    <TaxiLight Ignored="True" />
    <StrobeLights Ignored="True" />
    <LandingLights Ignored="True" />
    <Flaps>
      <Flap Index="0" Text="UP" />
      <Flap Index="1" Text="CONF 1" />
      <Flap Index="2" Text="CONF 1+F" />
      <Flap Index="3" Text="CONF 2" />
      <Flap Index="4" Text="CONF 3" />
      <Flap Index="5" Text="FULL" />
    </Flaps>
  </Rule>
</Rules>
